{
  "name": "sveltekit-boilerplate",
  "version": "1.0.0",
  "author": {
    "name": "BruhaBruh",
    "url": "https://github.com/BruhaBruh"
  },
  "scripts": {
    "dev": "env-cmd svelte-kit dev",
    "build": "env-cmd -f .env.json svelte-kit build ",
    "package": "svelte-kit package",
    "preview": "env-cmd svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "vitest --passWithNoTests",
    "test:watch": "vitest watch",
    "typesafe-i18n": "typesafe-i18n"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@sika7/sanitize-html": "^0.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "^1.0.0-next.316",
    "@twurple/auth": "^5.1.6",
    "@twurple/pubsub": "^5.1.6",
    "@types/chroma-js": "^2.1.3",
    "@types/tmi.js": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "chroma-js": "^2.4.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.12",
    "tmi.js": "^1.8.5",
    "tslib": "^2.3.1",
    "typesafe-i18n": "^5.4.0",
    "typescript": "~4.6.2",
    "vitest": "^0.8.3"
  },
  "type": "module",
  "dependencies": {}
}
