{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "@i18n": ["src/i18n"],
      "@i18n/*": ["src/i18n/*"],
      "$types": ["src/types"],
      "$types/*": ["src/types/*"]
    }
  }
}
